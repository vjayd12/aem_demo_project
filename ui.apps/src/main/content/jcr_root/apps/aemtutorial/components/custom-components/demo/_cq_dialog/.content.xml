<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Dialog"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <single-card
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Single Card"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <max-cards
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                description="Please Enter the Number that be Showed"
                                                fieldLabel="Enter the Number Show "
                                                name="./numbers"
                                                step="1" />
                                            <noblogfound
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Enter the Title"
                                                maxlength="12"
                                                name="./title" />
                                            <paragraph
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                description="Please Enter the description of the field"
                                                fieldLabel="Enter the Description"
                                                name="./description"
                                                required="true" />
                                            <genderselection
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Select Gender"
                                                name="./gendertype">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <option1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Male"
                                                        value="male" />
                                                    <option2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Female"
                                                        value="female" />
                                                    <option3
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Others"
                                                        value="others" />
                                                </items>
                                            </genderselection>
                                            <redirectionLink
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                fieldLabel="Enter the Redirection Path For CTA "
                                                name="./redirectlink"
                                                roothPath="/content/aemtutorial" />
                                            <image
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldLabel="Enter Featured Image Path"
                                                name="./image"
                                                rootPath="/content/dam/" />
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </single-card>
                    <multi-cards
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Multi Cards"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">

                                            <cardmultifiedld
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Card Multifield">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                    name="./sourcecardField">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <max-cards
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                                    description="Please Enter the Number that be Showed"
                                                                    fieldLabel="Enter the Number Show "
                                                                    name="./cardnumbers"
                                                                    step="1" />
                                                                <noblogfound
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Enter the Title"
                                                                    maxlength="12"
                                                                    name="./cardtitle" />
                                                                <paragraph
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                                    description="Please Enter the description of the field"
                                                                    fieldLabel="Enter the Description"
                                                                    name="./carddescription"
                                                                    required="true" />
                                                                <genderselection
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                    fieldLabel="Select Gender"
                                                                    name="./cardgendertype">
                                                                    <items
                                                                        jcr:primaryType="nt:unstructured">
                                                                        <option1
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="Male"
                                                                            value="male" />
                                                                        <option2
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="Female"
                                                                            value="female" />
                                                                        <option3
                                                                            jcr:primaryType="nt:unstructured"
                                                                            text="Others"
                                                                            value="others" />
                                                                    </items>
                                                                </genderselection>
                                                                <redirectionLink
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                                    fieldLabel="Enter the Redirection Path For CTA "
                                                                    name="./cardredirectlink"
                                                                    roothPath="/content/aemtutorial" />
                                                                <image
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                    fieldLabel="Enter Featured Image Path"
                                                                    name="./cardimage"
                                                                    rootPath="/content/dam/" />
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </cardmultifiedld>

                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </multi-cards>

    <multi-cards
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Multi Cards"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">

                                      <styletab
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/mnt/overlay/cq/gui/components/authoring/dialog/style/tab_design/styletab"/>

                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </multi-cards>
                </items>
            </tabs>

        </items>
    </content>
</jcr:root>